      <message><![CDATA[Введите тип пропуска]]></message>
    </field-validator>
  </field>
  <field name="Наименование организации"/>
  <field name="Вид выполняемых работ">
    <field-validator type="required">
      <message><![CDATA[Введите вид работ ]]></message>
    </field-validator>
  </field>
  <field name="Время окончания работ">
    <field-validator type="required">
      <message><![CDATA[Введите время окончания работ]]></message>
    </field-validator>
  </field>
  <field name="Время начала">
    <field-validator type="required">
